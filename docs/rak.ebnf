
chunk             ::= stmt* EOF

stmt              ::= expr ";"

expr              ::= cmp_expr ( ( "==" | "!=" ) cmp_expr )*

cmp_expr          ::= add_expr ( ( ">=" | ">" | "<=" | "<" ) add_expr )*

add_expr          ::= mul_expr ( ( "+" | "-" ) mul_expr )*

mul_expr          ::= unary_expr ( ( "*" | "/" | "%" ) unary_expr )*

unary_expr        ::= "-" unary_expr | prim_expr

prim_expr         ::= "nil" | "false" | "true" | NUMBER 
                    | "(" expr ")"
